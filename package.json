{
	"name": "gravitate-agent",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"tunnel:login": "cloudflared tunnel login",
		"tunnel:create": "cloudflared tunnel create gravitate-agent",
		"tunnel:dns": "cloudflared tunnel route dns gravitate-agent dev.yourdomain.com",
		"tunnel:run": "cloudflared tunnel --config cloudflared/config.yml run gravitate-agent",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
