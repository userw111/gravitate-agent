{
	"name": "gravitate-agent",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"tunnel:login": "cloudflared tunnel login",
		"tunnel:create": "cloudflared tunnel create gravitate-agent",
		"tunnel:dns": "cloudflared tunnel route dns gravitate-agent dev.yourdomain.com",
		"tunnel:run": "cloudflared tunnel --config cloudflared/config.yml run gravitate-agent",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.11.1",
		"@workos-inc/node": "^7.72.2",
		"convex": "^1.16.0",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.46.0"
	}
}